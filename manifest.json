{
  "name": "z-music.org sortable",
  "version": "0.1.0",
  "description": "Allows to sort search results by drag-n-drop.",
 "content_scripts" : [
    {
      "matches" : [
        "http://z-music.org/*",
      ],
      "js" : ["contentscript.js"],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ],
  "permissions": [
    "http://z-music.org/",
  ],
}
